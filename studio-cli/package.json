{
  "name": "@typedb/studio",
  "version": "0.1.0",
  "description": "TypeDB Web Studio - Browser-based IDE for TypeDB with embedded WASM database",
  "license": "MPL-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/typedb/typedb"
  },
  "keywords": [
    "typedb",
    "database",
    "studio",
    "ide",
    "wasm",
    "graph-database"
  ],
  "bin": {
    "typedb-studio": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./mod.ts",
      "import": "./mod.ts"
    },
    "./server": {
      "types": "./server.ts",
      "import": "./server.ts"
    }
  },
  "files": [
    "bin/",
    "dist/",
    "mod.ts",
    "server.ts",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prepublishOnly": "echo 'Run build:cli from parent directory first'"
  }
}
